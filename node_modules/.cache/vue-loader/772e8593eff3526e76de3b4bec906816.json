y>\n          <v-list nav dense>\n            <v-list-item-group v-model=\"group\" active-class=\"deep-purple--text text--accent-4\">\n              <v-list-item>\n                <v-list-item-icon>\n                  <v-icon>mdi-home</v-icon>\n                </v-list-item-icon>\n                <v-list-item-title>Home</v-list-item-title>\n              </v-list-item>\n\n              <v-list-item>\n                <v-list-item-icon>\n                  <v-icon>mdi-account</v-icon>\n                </v-list-item-icon>\n                <v-list-item-title>Account</v-list-item-title>\n              </v-list-item>\n            </v-list-item-group>\n          </v-list>\n        </v-navigation-drawer> -->\n        <v-card-title class=\"mx-auto\" justify-center >ตะกร้าสินค้าของฉัน</v-card-title>\n        <v-card-text class=\"pa-0\" style=\"height: 100vh\">\n          <v-container elevation-0>\n            <v-row align=\"center\" v-for=\"(value, key, index) in group_data\" :key=\"index\">\n              <v-col><v-img :src=\"value[0]['file_img']\" max-width=\"200px\"></v-img></v-col>\n              <v-col> {{ key }}</v-col>\n              <v-col\n                ><v-text-field\n                  type=\"number\"\n                  append-outer-icon=\"mdi-add\"\n                  @click:append-outer=\"increment(value[0])\"\n                  prepend-icon=\"mdi-remove\"\n                  @click:prepend=\"decrement(value[0])\"\n                  v-model=\"value.length\"\n                ></v-text-field>\n              </v-col>\n            </v-row>\n          </v-container>\n          <v-divider></v-divider>\n          <v-container elevation-0 class=\"mx-auto\" justify=\"center\">\n            <!-- <v-row class=\"mx-auto\" justify=\"center\"> -->\n                <v-card outlined >\n                    <v-card-title   class=\"text-center\">\n                        Payment Summary\n                    </v-card-title>\n                \n                <v-list>\n                <v-list-item>\n                    <v-list-item-content>\n                    <span>รวม </span> \n                    </v-list-item-content>\n                    <v-list-item-action>\n                        {{ number_book }} เล่ม\n                    </v-list-item-action>\n                    </v-list-item>\n                <v-list-item>\n                    <v-list-item-content>\n                    <span>ราคารวม </span> \n                    </v-list-item-content>\n                    <v-list-item-action>\n                        {{ total_price }} บาท\n                    </v-list-item-action>\n                </v-list-item>\n                <v-list-item>\n                    <v-list-item-content>\n                    <span>ส่วนลด </span> \n                    </v-list-item-content>\n                    <v-list-item-action>\n                        {{ promo }} บาท\n                    </v-list-item-action>\n                </v-list-item>\n                <v-list-item>\n                    <v-list-item-content>\n                    <span>รวมสุทธิ </span> \n                    </v-list-item-content>\n                    <v-list-item-action>\n                       {{ net_price }} บาท\n                    </v-list-item-action>\n                </v-list-item>\n              </v-list>\n              </v-card>\n            <!-- </v-row> -->\n          </v-container>\n        </v-card-text>\n      </v-card>\n    </v-dialog>\n  </v-row>\n</template>\n<script>\nimport { mdiAccount } from '@mdi/js'\nexport default {\n  props: [\"show\", \"data_select\"],\n  data: function () {\n    return {\n      drawer: false,\n      group: null,\n      data_in_cart: [],\n      group_data: {},\n      number_book: 0,\n      total_price: 0,\n      promo: 0,\n      net_price: 0,\n      data_discount_1: [],\n      data_discount_number: [],\n    };\n  },\n  computed: {\n    shower: {\n      get() {\n        if (this.show === true) {\n          console.log(\"cart\", this.data_select);\n        }\n        return this.show;\n      },\n      set(value) {\n        if (!value) {\n          //   this.$emit(\"close\");\n        }\n      },\n    },\n  },\n  watch: {\n    show: function () {\n      this.calculateBook();\n    },\n  },\n  methods: {\n    increment(value) {\n      for (let i in this.group_data) {\n        if (value.file_name === i) {\n          this.group_data[value.file_name].push(value);\n          this.data_discount = console.log(this.group_data);\n        }\n      }\n      this.calPriceBook();\n    },\n    decrement(value) {\n      for (let i in this.group_data) {\n        if (value.file_name === i) {\n          this.group_data[value.file_name].pop(value);\n          console.log(this.group_data[value.file_name].length);\n          if (this.group_data[value.file_name].length === 0) {\n            delete this.group_data[value.file_name];\n          }\n        }\n      }\n      this.calPriceBook();\n    },\n\n    calculateBook() {\n      this.group_data = this.data_select.reduce((r, a) => {\n        r[a.file_name] = [...(r[a.file_name] || []), a];\n        return r;\n      }, {});\n      console.log(\"group\", this.group_data);\n      this.calPriceBook();\n    },\n    calPriceBook() {\n      let n = 0;\n      this.number_book = 0;\n      this.total_price = 0;\n      this.promo = 0;\n      this.net_price = 0;\n      this.data_discount_1 = [];\n      this.data_discount_number = [];\n      for (let i in this.group_data) {\n        this.number_book += this.group_data[i].length;\n\n        this.data_discount_number[n] = this.group_data[i].length;\n        n += 1;\n        console.log(this.group_data[i]);\n      }\n      console.log(\"gh\", this.data_discount_number);\n      let m = 0;\n      let c = 1;\n      let tmp_length = this.data_discount_number.length;\n      for (let i = 0; i < tmp_length; i++) {\n        if (this.data_discount_number[i] > 0) {\n          this.data_discount_number[i] -= 1;\n          console.log(!this.data_discount_1[m]);\n\n          this.data_discount_1[m] = c;\n          c += 1;\n        }\n        console.log(i);\n        if (i === tmp_length - 1) {\n            console.log(\"เข้า\");\n          m += 1;\n        }\n        this.data_discount_number[i];\n      }\n      console.log(this.data_discount_1);\n      //console.log(this.data_discount);\n      this.total_price = this.number_book * 100;\n      this.promo = (10 / 100) * (n * 100);\n      this.net_price = this.total_price - this.promo;\n    },\n  },\n};\n</script>\n<style></style>\n"]}]}